{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": "off",
        "no-dupe-else-if": ["error"],
        "eqeqeq": ["warn", "always"],
        "no-implicit-globals": "error",
        "no-redeclare": "error",
        "no-promise-executor-return": "error",
        "no-undef": "off",
        "no-unused-vars": "off",
        "arrow-spacing": "warn",
        "no-var": "error",
        "prefer-const": "error",
        "no-unreachable": "error",
        "complexity": ["warn", 10],
        "no-invalid-this": "warn",
        "no-multi-spaces": "warn",
        "no-self-assign": "warn",
        "no-shadow": "warn",
        "comma-spacing": "warn",
        "max-lines-per-function": ["warn", 75],
        "prefer-object-spread": "warn",
        "prefer-template": "warn"
    }
}
